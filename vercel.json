{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://asdp-fba8.onrender.com/$1"
    },
    {
      "src": "/(login|register|logout|me|upload|clean|report|download_data|admin|profile|avatars|healthz|test|deploy-test|deployment-status)",
      "dest": "https://asdp-fba8.onrender.com/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html"
    }
  ]
}
